			@Override
			public ResultSet getResultSet(Map<String, Class<?>> map) throws SQLException {
				// TODO Auto-generated method stub
				return null;
			}
			
			@Override
			public ResultSet getResultSet() throws SQLException {
				// TODO Auto-generated method stub
				return null;
